{
  "version": 2,
  "builds": [
    {
      "src": "census/app/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/census/app/$1"
    }
  ]
}
